    "install_all": "npm install --no-optional --no-shrinkwrap --no-package-lock",
    "dev": "webpack --mode development  --config webpack.development.config.ts",
    "build": "webpack --mode production  --config webpack.production.config.ts",
    "reinstall": "rm -f package-lock.json && rm -rf node_modules && npm install",
    "test": "jest",
    "eslint": "eslint \"**/*.{ts,tsx}\" --ignore-pattern node_modules/"
